    "def policy_evaluation(policy, states, threshold = 0.0000001):\n",
    "    V_prime = np.zeros(len(states))\n",
    "    values = np.ones(len(states)) * np.Inf\n",
    "    while np.any(np.abs(V_prime - values) > threshold):\n",
    "        values = copy.deepcopy(V_prime)\n",
    "        for state in states:\n",
    "            P = transition_prob(state)\n",
    "            V_prime[state] = np.dot(values + r, P[:, policy[state]])\n",
    "    return values\n",
    "\n",
    "def policy_improvement(states, policy, values):\n",
    "    better_policy = copy.deepcopy(policy)\n",
    "    for state in states:\n",
    "        P = transition_prob(state)\n",
    "        better_policy[state] = np.argmax(np.dot(values + r, P))\n",
    "    return better_policy\n",
    "\n",
    "def policy_iteration(states):\n",
    "    policy = [1] * len(states)\n",
    "    policy[0] = 0\n",
    "    policy[100] = 0\n",
    "    while True:\n",
    "        values = policy_evaluation(policy, states)\n",
    "        better_policy = policy_improvement(states, policy, values)\n",
    "        if better_policy == policy:\n",
    "            break\n",
    "        policy = better_policy\n",
    "    \n",
    "    values = policy_evaluation(policy, states)\n",
    "    return values, policy\n",
    "\n",
    "values, policy = policy_iteration(states)"
   ]
